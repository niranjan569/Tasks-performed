<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">  
   
   <context:annotation-config />

	<!--Define beans that we have created-->
	<bean id="customer" class="com.annotation.Customer"/>
	<bean id="person" class="com.annotation.Person">
		<property name="pName" value="Niranjan"/>
		<property name="pAge" value="22" />
	</bean>
	
	<bean id="person1" class="com.annotation.Person">
		<property name="pName" value="Niroop"/>
		<property name="pAge" value="22" />
	</bean>
</beans>