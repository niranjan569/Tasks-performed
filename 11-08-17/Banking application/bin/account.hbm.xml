<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"../Banking application/hibernate-mapping-3.0.dtd">
<!-- Hibernate mapping file -->
<hibernate-mapping>
	<class name="com.model.Account">
		<id name="pAccountNo">
			<column name="AccountNo"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="pName">
			<column name="Name"></column>
		</property>
		<property name="pBalance">
			<column name="Balance"></column>
		</property>
	</class>
</hibernate-mapping>
